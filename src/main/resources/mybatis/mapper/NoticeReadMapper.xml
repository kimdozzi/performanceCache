<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ll.performancecache.mapper.NoticeReadMapper">

    <select id="findAll" resultType="com.ll.performancecache.dto.Notice">
        SELECT *
        FROM study_db.notice
        limit 5000;
    </select>

    <select id="findByPage" parameterType="Integer" resultType="com.ll.performancecache.dto.Notice">
        SELECT *
        FROM study_db.notice
        LIMIT #{startIdx},10;
    </select>

</mapper>